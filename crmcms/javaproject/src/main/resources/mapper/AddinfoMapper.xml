<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.crm.mapper.AddinfoMapper">

    <select id="qryAddiByno" resultType="com.neusoft.crm.entity.Addinfo">
        SELECT addinfo.*
        FROM addinfo
        WHERE addinfo.addrno=#{addrno}
    </select>
</mapper>
